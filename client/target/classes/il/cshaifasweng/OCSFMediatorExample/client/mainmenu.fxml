<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainmenuAnchor" prefHeight="459.0" prefWidth="445.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.MainMenuController">
   <children>
      <Label fx:id="hi_label" layoutX="287.0" layoutY="13.0" prefHeight="17.0" prefWidth="138.0" style="-fx-background-color: lightblue;" text="Welcome &lt;USER&gt;" />
      <Button fx:id="logoutButton" layoutX="14.0" layoutY="400.0" mnemonicParsing="false" onAction="#logout" prefHeight="25.0" prefWidth="86.0" text="Log Out" />
      <Button fx:id="tasksListButton" layoutX="160.0" layoutY="114.0" mnemonicParsing="false" onAction="#switchToTasks" prefHeight="25.0" prefWidth="126.0" text="Tasks List" />
      <AnchorPane fx:id="mainmenu_anchor_manager" layoutX="125.0" layoutY="191.0" prefHeight="169.0" prefWidth="197.0">
         <children>
            <Button fx:id="histogramButton" layoutX="60.0" layoutY="24.0" mnemonicParsing="false" onAction="#switchToHistograms" text="Histograms" />
            <Button fx:id="communityButton" layoutX="48.0" layoutY="72.0" mnemonicParsing="false" onAction="#switchToCommunityInfo" text="Community info" />
         </children>
      </AnchorPane>
      <Button fx:id="managerButton" layoutX="160.0" layoutY="157.0" mnemonicParsing="false" onAction="#manager_options_pressed" prefHeight="25.0" prefWidth="126.0" text="Manager Options" />
      <Button fx:id="emergencyButton" layoutX="155.0" layoutY="342.0" mnemonicParsing="false" onAction="#emergency_button_press" prefHeight="103.0" prefWidth="137.0" style="-fx-background-radius: 20; -fx-border-radius: 20; -fx-background-color: red;" text="Emergency Button" textFill="WHITE" />
   </children>
</AnchorPane>
